@{ 
    Layout = null;
}
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>admin</title>
    <script src="~/jquery-ajax-unobtrusive/jquery-1.7.1.min.js"></script>
    <script src="~/jquery-ajax-unobtrusive/JavaScript.js"></script>
    <link href="~/css/nullStyle.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
    <link href="~/css/adminPanel.css" rel="stylesheet" />
    <script src="~/js/admin.js"></script>
</head>
<body>
    <div class="userInfo">
        <div class="userInfo__content">
            
        </div>
    </div>
    <div class="admin">
        <div class="admin__container">
            <div class="admin__header">
                <div admin-type="applications" class="text3 admin__header__item">
                    Все заявки
                </div>
                <div admin-type="users" class="text3 admin__header__item">
                    Пользователи
                </div>
            </div>
            <div class="admin__content">
                <div class="admin__content__items _active__adminMenu" id="applications">
                    <div class="search">
        <div class="application__filtration__params">
            <div class="content__search__rangeCost">
                <div class="content__search__rangeCost__values">
                    <span class="text3">Кубки от</span>
                    <span class="text3" id="content__search__rangeCost__range1">0</span>
                    <span class="text3">Кубки до </span>
                    <span class="text3" id="content__search__rangeCost__range2">1250</span>
                </div>
                <div class="content__search__rangeCost__container">
                    <div class="slider-track"></div>
                    <input name="minCups" type="range" min="0" max="1250" value="0" id="slider-1" oninput="slideOne()">
                    <input name="maxCups" type="range" min="0" max="1250" value="1250" id="slider-2" oninput="slideTwo()">
                </div>
            </div>
                <select class="text3" id="cupsAscending">
                    <option value="true">кубки по возрастанию</option>
                    <option value="false">кубки по убыванию</option>
                </select>
            <div class="filtration_params__input">
                <input type="text" class="text3" id="application__searchString" />
                <input type="button" class="text3" name="Поиск" value="Поиск" onclick="submitParamsAdmin()" />
                </div>
        </div>
                    </div>
                    <div class="admin__items__applications">

                    </div>
                </div>
                <div class="admin__content__items" id="users">
                    <div class="user__filtration__params">
                        <div class="filtration_params__input">
                            <input type="text" class="text3" id="user__searchString" />
                            <input type="button" class="text3" name="Поиск" value="Поиск" onclick="getUsers()" />
                        </div>
                    </div>
                    <div class="admin__items__users">

                    </div>
                </div>
            </div>
        </div>
    </div>
   <script src="~/js/slider.js"></script>
            <script src="~/js/sendFiltrationParams.js"></script>
            <link href="~/css/search.css" rel="stylesheet" />
</body>
</html>